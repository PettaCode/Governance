{
    "name": "nova-framework/framework",
    "type": "project",
    "description": "Framework for PHP 5.5+",
    "homepage": "https://github.com/nova-framework/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@daveismyname.com",
            "homepage": "http://daveismyname.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/console": "^3.1",
        "symfony/finder": "^3.1",
        "symfony/http-foundation": "^3.1",
        "symfony/http-kernel": "^3.1",
        "symfony/routing": "^3.1",
        "stack/builder": "^1.0",
        "doctrine/inflector": "1.0.*",
        "patchwork/utf8": "~1.2",
        "nesbot/carbon": "^1.21",
        "swiftmailer/swiftmailer": "^5.4",
        "monolog/monolog": "^1.19",
        "predis/predis": "0.8.*",
        "filp/whoops": "^2.1",
        "almasaeed2010/adminlte": "~2.0",
        "twbs/bootstrap": "^3.3"        
    },
    "autoload": {
        "psr-4": {
            "App\\" : "app/",
            "Shared\\" : "shared/",
            "" : "system/"
        },
        "files": [
            "system/functions.php"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
